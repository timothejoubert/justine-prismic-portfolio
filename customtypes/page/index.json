{
  "id": "page",
  "label": "Page",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "Slug",
          "placeholder": "Le nom de ta page dans l'url"
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": "Titre de la page"
        }
      },
      "description": {
        "type": "StructuredText",
        "config": {
          "multi": "paragraph,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink",
          "allowTargetBlank": true,
          "label": "Description",
          "placeholder": "Description de la page"
        }
      },
      "thumbnail": {
        "type": "Image",
        "config": {
          "constraint": {},
          "thumbnails": [],
          "label": "Thumbnail"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "loading_image": {
              "type": "SharedSlice"
            },
            "contact_block": {
              "type": "SharedSlice"
            },
            "project_listing_block": {
              "type": "SharedSlice"
            },
            "about_block": {
              "type": "SharedSlice"
            },
            "project_push": {
              "type": "SharedSlice"
            },
            "sketch_book": {
              "type": "SharedSlice"
            }
          }
        }
      }
    }
  }
}